{
  "version": "v1",
  "port": "9000",
  "scheme":"http",
  "middleware":{
    "auth":"./plugins/token.so",
    "sign":"./plugins/sign.so"
  },
  "rules": {
    "/test/data":{
        "filters":[],
        "aggregate":{
            "data.user.posts":{
                "Service":"jsonplaceholder.typicode.com",
                "Endpoint":"/posts/1"
            },
            "data.user.users":{
                "Service":"jsonplaceholder.typicode.com",
                "Endpoint":"/users/1"
            }
        }
    },
    "/game/init":{
        "filters":[
        ],
        "aggregate":{
            "version":{
                "Service":"version-service",
                "Endpoint":"/init"
            },
            "GeneralData":{
                "Service":"general-service",
                "Endpoint":"/init"
            },
            "Money":{
                "Service":"money-service",
                "Endpoint":"/init"
            },
            "Player":{
                "Service":"player-service",
                "Endpoint":"/init"
            },
            "Levels":{
                "Service":"levels-service",
                "Endpoint":"/init"
            },
            "Inventory":{
                "Service":"inventory-service",
                "Endpoint":"/init"
            },
            "SocialData":{
                "Service":"social-service",
                "Endpoint":"/init"
            },
            "Flags":{
                "Service":"flags-service",
                "Endpoint":"/init"
            },
            "Chests":{
                "Service":"chest-service",
                "Endpoint":"/init"
            },
            "Offers":{
                "Service":"offers-service",
                "Endpoint":"/init"
            },
            "AbTests":{
                "Service":"ab-service",
                "Endpoint":"/init"
            }
        }
    },
    "/auth":{
        "service":"auth-service",
        "filters":[
        ]
    }
  },
  "not_found_error": {
    "code": 404,
    "domain": "Route not found",
    "message": "The requested route was not found"
  },
  "fallback_rule": "www.openenergi.com"
}
